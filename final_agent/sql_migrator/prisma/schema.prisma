// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-py"
}

// January
model Jan2025 {
  id          Int      @id @default(autoincrement())
  time        String
  lat         Float?
  lon         Float?
  pressure    Float?
  temperature Float?
  salinity    Float?

 
}

